component pss_top {
    action test {
        // Test declaration
        set<int> my_set1;
        set<int> my_set2;
        set<int> my_set3;
        set<string> my_set4;
        list<int> my_set2_list;

        // Test assignment
        exec post_solve {
            my_set1 = {53, 52, 51};
            my_set2 = {51, 52, 53, 52};
            my_set3.insert(1);
            my_set3.insert(52);
            my_set3.insert(51);
            my_set3.insert(53);
            my_set3.delete(1);
            my_set4 = {"1", "2", "2"};
            my_set2_list = my_set2.to_list();
        }

        // Test index
        activity {
            if (52 in my_set1)
			    do printf with {val == 52;};
            if (my_set1 == my_set2)
			    do printf with {val == 1;};
            if (my_set1 != my_set2)
			    do printf with {val == -1;}; // -1 should not be printed
			do printf with {val == my_set2.size();};
            if (my_set1 == my_set3)
			    do printf with {val == 1;};
			do printf with {val == my_set4.size();};
            if ("3" in my_set4)
			    do printf with {val == -1;}; // -1 should not be printed
            do printf with {val == my_set2_list.size();};
        }
    }

    action printf {
        rand int val;
        exec body ASM =
        """val = {{val}}""";
    }

    action root_a {
        activity {
            do test;
        }
    }
}

